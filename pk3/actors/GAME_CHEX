    States
    {
      Spawn:
        HEAD A 10 A_Look
        Loop

      See:
        HEAD A 3 A_Chase
        Loop

      Missile:
        TNT1 A 0 A_ChangeFlag("NOPAIN",0)
        HEAD B 5 A_FaceTarget
        HEAD C 5 A_FaceTarget
        HEAD D 5 A_CustomComboAttack("CycloptisBall", 32, random[headattack](1,6)*10, "", "flem")
        Goto See

      Pain:
        HEAD A 3 A_SpawnDebris("FlemoidGib",1)
        HEAD A 3 A_Pain
        HEAD A 6
        Goto See

      Pain.ZorchPistol:
      Pain.Zorch:
      Pain.PhasingZorch:
