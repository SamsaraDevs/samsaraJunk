ACTOR SlimeGrenade
{
   Radius 8
   Height 8
   Speed 25
   FastSpeed 35
   Damage 10
   +NOBLOCKMAP 
   +DROPOFF 
   +MISSILE 
   +ACTIVATEIMPACT 
   +ACTIVATEPCROSS
   +DONTSPLASH
   +DOOMBOUNCE
   -EXTREMEDEATH
   BounceFactor 0.95
   Scale 0.75
   BounceCount 4
   DamageType "Flem"
   SeeSound "grunt/attack"
   DeathSound "flemdie"
   States
   {
   Spawn:
      BAL2 AB 3
      Loop
   Death:
      TNT1 AAAAAAAAAAAAAAAAAAAAAA 0 A_SpawnItemEx("FlemoidGib",random(-1,1),random(-1,1),random(-1,1),random(-2,2),random(-2,2),random(5,10))
      BAL2 C 4 A_Explode(64,96)
      BAL2 D 4
      BAL2 E 4
      Stop
   }
}

