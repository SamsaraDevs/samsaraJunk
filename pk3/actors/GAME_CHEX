        HEAD E 3
        HEAD E 3 A_Pain
        HEAD F 6
        Goto See

      Death.ZorchPistol:
      Death.Zorch:
      Death.PhasingZorch:
      Death.ZorchKamikaze:
        TNT1 A 0 A_GiveToTarget("KillCount",1)
        HEAD G 8
        HEAD H 8 A_Scream
        HEAD I 8
        HEAD J 8
        HEAD K 8 A_NoBlocking
        HEAD L -1 A_SetFloorClip
        Stop

      Death.DukeExplosive:
      Death.DukeDamage:
      Death.DukePistol:
        TNT1 A 0 A_Jump(218,"Death")
        TNT1 A 0 A_GiveToTarget("DukeKillTaunt",1)
        goto Death

