        TNT1 A 0 A_NoBlocking
        HEAD A 0 A_UnsetSolid
        HEAD A 70 A_GenericFreezeDeath
        HEAD A 0 A_IceGuyDie
        HEAD A 1 A_IceGuyDie
        TNT1 A -1
        stop

      Pain.MightyBoot:
        TNT1 A 0 A_ChangeFlag("NOPAIN",1)
        TNT1 A 0 A_PlaySoundEx("duke/kickhit","body",0,0)
